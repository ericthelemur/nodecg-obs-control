{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "inputKind": {
      "type": [
        "null",
        "string"
      ]
    },
    "isGroup": {
      "type": [
        "boolean",
        "null"
      ]
    },
    "sceneItemBlendMode": {
      "type": "string",
      "enum": [
        "OBS_BLEND_NORMAL",
        "OBS_BLEND_ADDITIVE",
        "OBS_BLEND_SUBTRACT",
        "OBS_BLEND_SCREEN",
        "OBS_BLEND_MULTIPLY",
        "OBS_BLEND_LIGHTEN",
        "OBS_BLEND_DARKEN"
      ]
    },
    "sceneItemEnabled": {
      "type": "boolean"
    },
    "sceneItemId": {
      "type": "integer"
    },
    "sceneItemIndex": {
      "type": "integer"
    },
    "sceneItemLocked": {
      "type": "boolean"
    },
    "sceneItemTransform": {
      "$ref": "obs_transform.json"
    },
    "sourceName": {
      "type": "string"
    },
    "sourceType": {
      "type": "string"
    }
  },
  "required": [
    "inputKind",
    "isGroup",
    "sceneItemBlendMode",
    "sceneItemEnabled",
    "sceneItemId",
    "sceneItemIndex",
    "sceneItemLocked",
    "sceneItemTransform",
    "sourceName",
    "sourceType"
  ]
}